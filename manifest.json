{
  "manifest_version": 2,
  "name": "CSS DarkTheme",
  "description": "This extension set dark theme for diferents pages",
  "version": "0.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "background": {
    "scripts": ["./assets/bg.js"]
  }
}
